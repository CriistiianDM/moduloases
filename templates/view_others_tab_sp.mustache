                        <textarea required rows="10" cols="30" class=" academics_data_textarea" id="textarea_academics_dificultades" placeholder="Dificultades que presentó">{{academics_dificults}}</textarea>
                    </div>
                </div>
            </div>

            <div hidden>
                <input disabled class="input_fields_discapacity_tab" type="text" id="input_academics_saved" value="{{academics_data}}" />
            </div>

            <div hidden>
                <input disabled class="input_fields_discapacity_tab" type="text" id="input_json_academics_saved" value="{{academics_data_json}}" />
            </div>

