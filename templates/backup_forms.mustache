 {{#data}} {{> block_ases/side_menu}} {{#backup_forms_bf}}
<div class="container">

  <h2 id="title-view">REPORTE DE BACKUP</h2>
  <div id="toogle-selector" >
    Simplificado
    <label class="switch">
      <input type="checkbox">
      <span class="slider round" data-status="off" ></span>
    </label>
    Avanzado
  </div>
  <hr style="background-color: red; height: 1px; border: 0">
  <div id="busqueda-simplificada" class="row">

    <div class="col-xs-5 col-sm-5 col-md-5 col-lg-3">
      <div class="form-group">
          <label for="criteria_select">Buscar por:</label>
          <select id="simple_criteria_select" name="simple_criteria_select" class="info_backup" style="width: 100%;">
              <option value="true">Estudiante</option>
              <option value="false">Monitor</option>
              <option value="false">Practicante</option>
              <option value="false">Profesional</option>
            </select>
      </div>
    </div>
    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
      <div class="form-group">
          <label for="">Código/Username:</label>
          <input id="simple_cod_user" name="simple_cod_user" class="form-control info_backup" type="text">
      </div>
    </div>
    <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 col-xs-offset-4 col-sm-offset-4 col-md-offset-4 col-lg-offset-4">
      <label for="" style="color:white;">.</label><br>
      <input class="btn-danger btn-generar-filtro" type="button" id="generarFiltroSimplificado" value="Buscar">
    </div>

  </div>
  <div id="busqueda-avanzada" style="display:none;" class="row">
    <div class="row">
      <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 ">
        <input class= "info_backup" id="filtroCodigoUsuario" name="idFiltro" type="text" class="form-control" placeholder="Código de usuario"  >
      </div>
       <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 " style="padding-left: 25px">
        <select id="selectTable"  class= "info_backup" title="Seleccione el tipo de consulta">
            <option value="talentospilos_user_extended" title="Para cualquier estudiante ASES">Id ases</option>
            <option value="user" title="Para cualquier usuario moodle">Id moodle</option>
        </select>
      </div>
  
      <div id="div_code_user" class="col-lg-2 col-md-2 col-sm-2 col-xs-2 alert alert-danger div_info_user">
        <strong>Usuario: </strong> Moodle/Ases
      </div>
  
      <div id="div_name_user" class="col-lg-4 col-md-4 col-sm-4 col-xs-4 alert alert-danger div_info_user">
        <strong> Nombre: </strong> Nombre
      </div>
  
  
      <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
        <input class="btn-danger" type="button" id="generarFiltro" value="Buscar">
      </div>
    </div>
  
    <hr>
  
    <div class="row">
        <label class="col-lg-2 col-md-2 col-sm-2 col-xs-2 info_backup" for="typecolumn_select" style="font-size:14px; padding-top: 10px;">Atributo a consultar</label>
        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2" style="padding-left: 0px;padding-right: 0px">
    
          <select id="typecolumn_select" name="typecolumn_select" class="info_backup">
            <option value="id">Id</option>
            <option value="id_usuario_moodle">Id usuario moodle</option>
            <option value="accion">Acción</option>
            <option value="id_registro_respuesta_form">Id respuesta</option>
            <option value="datos_previos" selected="true">Datos previos</option>
            <option value="datos_enviados">Datos enviados</option>
            <option value="datos_almacenados">Datos almacenados</option>
            <option value="observaciones">Observaciones</option>
            <option value="cod_retorno">Código retorno</option>
            <option value="msg_retorno">Mensaje retorno</option>
            <option value="dts_retorno">Datos retorno</option>
            <option value="fecha_hora_registro">Fecha y hora</option>
            <option value="navegador">Navegador</option>
            <option value="url_request">Url</option>
          </select>
        </div>
        <div hidden>
            <label class="col-lg-2 col-md-2 col-sm-2 col-xs-2 info_backup" for="typekey_select" style="font-size:14px; padding-top: 10px;">Respuesta a consultar</label>
  
            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2" style="padding-left: 0px;padding-right: 0px" >
        
                <select id="typekey_select" class="info_backup">
                    {{{options_key_select}}}
                </select>
            </div>
        </div>
        
  
  
      </div>
  
      <hr>
    <div class="row">
      <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
        <input id="filtroLike" name="idFiltroLike" type="text" class="form-control info_backup" placeholder="Ingrese el texto a consultar" >
      </div>
      <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
        <input class="btn-danger" type="button" id="load_like" style=" width: 170px; height: 39px;margin-top: 0px;" value="Buscar">
      </div>
    </div>
  </div>

  <hr style="background-color: red; height: 1px; border: 0">

  <div class="panel panel-default" id="dwarehouse">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" href="#collapse_dwarehouse">
              Reporte dwarehouse</a>
      </h4>
    </div>
    <div id="collapse_dwarehouse" class="panel-collapse collapse">

      <div class="row table_logs">
        <div id="div_table_forms">
        </div>
      </div>
            
    </div>
  </div>

  <div class="panel panel-default" id="dwarehouse">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapse_general_logs">
                Reporte general logs</a>
        </h4>
      </div>
      <div id="collapse_general_logs" class="panel-collapse collapse">
        
      <div class="row table_logs">
        <div id="div_table_general_logs">
        </div>
      </div>

      </div>
    </div>
  


  <div class="mymodal" id="modal_JSON" name="modal_JSON">
    <div class="outside"></div>
    <div class="mymodal-content">

      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">
            <h2>Visualización avanzada de registro</h2>
          </h4>
        </div>
        <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" hidden>
            <a href="javascript:void(0)" class="btn btn-default btn-sm dphpforms-peer-record" data-record-id="-1">Mostrar</a>
            <a href="javascript:void(0)" class="btn btn-default btn-sm dphpforms-restore" data-record-id="-1">Restaurar</a>
            <a href="javascript:void(0)" class="btn btn-default btn-sm dphpforms-compare" data-record-id="-1">Comparar estados</a>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="div_JSONform">
            </div>
          </div>
        </div>
        <hr style="background-color: red; height: 1px; border: 0">
        <button id="btn-close-modal" class=" btn-danger-close  btn-block"> Cerrar </button>
      </div>
      <div class="mymodal-footer" style="text-align: center">
        .
      </div>
    </div>
  </div>
  

  <div class="mymodal" id="modal_v2_edit_peer_tracking" name="modal_edit_v2_pares">
    <div class="outside"></div>
    <div class="mymodal-content">
      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">Visualización/Edición</h4>
        </div>
        <div class="col-sm-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="body_editor"></div>
          </div>
        </div>
      </div>
      <div class="mymodal-footer">
        .
      </div>
    </div>
  </div>


  <div hidden="true">
    <input id="json_record_dwarehouse_selected" type="text" disabled = "true">
  </div>

  {{/backup_forms_bf}}
  <!--end panel-group-->
  {{/data}}
</div>