<div id="general_tab" class="tab-pane active p-t-100">
        <div class="ases-col-xs-12 ases-col-sm-7 ases-col-md-7 ases-col-lg-8">

            <div class="ases-row" id="div-edicion">
                <h3 class="ases-col-lg-5">INFORMACIÓN DEL MONITOR</h3>
                        
                <span id="span-icon-edit" class="ases-col-sm-1 ases-col-md-1 ases-col-lg-1 tooltip-v1" data-toggle="tooltip" data-placement="top" title="Editar ficha">
                </span>


                <span id="span-icon-save" class="ases-col-sm-1 ases-col-md-1 ases-col-lg-1 tooltip-v1" 
                      data-toggle="tooltip" data-placement="top" title="Guardar cambios">
                </span>
                        
                <span id="span-icon-cancel" class="ases-col-sm-1 ases-col-md-1 ases-col-lg-1 tooltip-v1" 
                    data-toggle="tooltip" data-placement="top" title="Cancelar cambios">
                </span>
            </div>

            <hr/>
                  <div class="ases-row">
                      <label class="ases-col-xs-3 ases-col-sm-3 ases-col-md-3 ases-col-lg-3" for="num_doc"> Identificación </label>
                      <input id="input_num_doc" name="num_doc" class="ases-col-xs-2 ases-col-sm-2 ases-col-md-2 ases-col-lg-2" readonly="" value={{num_doc}}>
                  </div>

                  <div class="ases-row">
                      <label class="ases-col-xs-3 ases-col-sm-3 ases-col-md-3 ases-col-lg-3" for="phone1"> Teléfono 1 </label>
                      <input id="input_phone1" name="phone1" class="ases-col-xs-2 ases-col-md-2 ases-col-lg-2" readonly="" value={{phone1}}>

                      <label class="ases-col-xs-2 ases-col-sm-2 ases-col-md-2 ases-col-lg-2" for="num_doc"> Teléfono 2 </label>
                      <input id="input_phone2" name="phone2" class="ases-col-xs-3 ases-col-sm-3 ases-col-md-3 ases-col-lg-3" readonly="true" value={{phone2}} >
                  </div>

                  <div class="ases-row">
                    <label class="ases-col-xs-3 ases-col-sm-3 ases-col-md-3 ases-col-lg-3" for="email">E-mail alternátivo </label>
                    <input id="input_email" name="email" class="ases-col-xs-7 ases-col-sm-7 ases-col-md-7 ases-col-lg-7" readonly="" value={{email_alter}} >
                </div>

                  <h5>Documentos</h5>
                  <div class="ases-row">
                      <label for="input_acuerdo" class="ases-col-xs-4 ases-col-sm-4 ases-col-md-4 ases-col-lg-4"> Acuerdo de Confidencialidad</label>
                        <a id="link_acuerdo" target="_blank" href={{pdf_acuerdo_conf}}><input type="text" id="input_acuerdo" name="pdf_acuerdo_conf" readonly="" value={{pdf_acuerdo_conf}}></a>
                  </div>

                  <div class="ases-row">
                      <label class="ases-col-xs-4 ases-col-sm-4 ases-col-md-4 ases-col-lg-4"> Documento de identidad </label>
                      <a id="link_doc" href={{pdf_doc}} target="_blank"><input type="text" name="pdf_doc" id="input_doc" id="input_doc" readonly="" value={{pdf_doc}}></a>
                  </div>

                  <div class="ases-row">
                      <label class="ases-col-xs-4 ases-col-sm-4 ases-col-md-4 ases-col-lg-4"> D-10 </label>
                      <a id="link_d10" href={{pdf_d10}} target="_blank"><input type="text" name="pdf_d10" id="input_d10" readonly="" value={{pdf_d10}}></a>
                  </div>

                  <div class="ases-row">
                      <label class="ases-col-xs-4 ases-col-sm-4 ases-col-md-4 ases-col-lg-4"> Cuenta bancaria </label>
                      <a id="link_banco" target="_blank" href="https://www.example.org"><input type="text" name="pdf_cuenta_banco" id="input_banco" readonly="" value="https://www.example.org"></a>
                  </div>
          </div>
</div>
