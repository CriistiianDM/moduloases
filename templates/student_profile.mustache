  </div>
  <!-- End modal Pimer Acercamiento-->
  <!-- Modal Segumiento Socioeducativo-->
  <div class="modal modal_pilos" id="myModalSegSocio" name="seguimiento" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h4 class="modal-title" id="myModalLabel">Seguimiento SocioEducativo</h4>
        </div>
        <div class="modal-body">
            <form class="form-horizontal" id = "SegSocioForm" role="form">
              <div class= "form-group hide" id="div_created">
                <div class="col-md-4">
                      <label type="text" class="" id="created_date" name="created_daydate"></label>
                </div>
              </div>
              <div class= "form-group">
                <div class="col-md-4">
                    <label for="date" class="form-control-label col-md-3">Fecha</label>
                    <div class="col-md-9">
                      <input type="date" class="form-control" id="date" name="date" >
                        <span >(dd/mm/aaaa)</span>
                      </input>
                    </div>
                </div>
              </div>
              <hr>
              <div class="form-group hide" id="infomonitor">
                <label for="monitor" class="form-control-label col-xs-2 col-sm-2">Profesional</label>
                <span class="col-sm-4" id="monitor"></span>
              </div>
              <hr> <hr>
              <div class= "form-group">
                <label for="motivo" class="form-control-label col-md-12">Motivo de Orientación</label>
                <div class="col-md-12">
                  <textarea  class="form-control medium-inline" id="motivo" name="motivo" maxlength="5000"></textarea>
                </div>
              </div>
              <hr>
              <div class= "form-group">
                <label for="freetime" class="form-control-label col-md-12">Seguimiento</label>
                <div class="col-md-12">
                  <textarea  class="form-control long-inline" id="seg" name="seg" maxlength="5000"></textarea>
                </div>
              </div>
              <hr>
              <div class="form-group" id="radio_div">
                <label for="actividades" class="form-control-label col-md-12">Calificación el nivel de riesgo</label>
                <div class="col-md-12" id="radio_div">
                  <label class="radio-inline" id="1">
                    <input type="radio" name="optradio"  value="1">Bajo
                  </label>
                  <label class="radio-inline" id="2">
                    <input type="radio" name="optradio" value="2">Medio Bajo
                  </label>
                  <label class="radio-inline" id="3">
                    <input type="radio" name="optradio" value="3">Medio
                  </label>
                  <label class="radio-inline" id="4">
                    <input type="radio" name="optradio" value="4">Medio Alto
                  </label>
                  <label class="radio-inline" id="5">
                    <input type='radio' name='optradio' value="5">Alto
                  </label>
                </div>
              </div>
            </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="button btn btn-secondary" data-dismiss="modal">Close</button>
          <button type="button" class="button btn btn-primary" id="save_seg">Save changes</button>
         <button type="button" class="button btn btn-primary hide" id="upd_seg">Actualizar</button>
        </div>
      </div>
    </div>
  </div>
  <!-- End modal Segumiento Socioeducativo-->
  <!-- Graphics Modal -->
  <div class="modal modal_pilos" id="myModalGraf" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h4 class="modal-title" id="myModalLabel"><b>GRÁFICA POR:</b></h4>
        </div>
        <div  id = "myModalBody">
           
        </div>
        <div class="modal-footer">
          <button type="button" class="button btn btn-secondary" data-dismiss="modal">Cerrar</button>
        </div>
      </div>
    </div>
  </div>
  <!-- End graphics Modal -->
  <!-- Modal retiro-->
  <div class="mymodal" id="modal_dropout" name="modal_dropout" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="mymodal-content">
        <div class="mymodal-header">
          <h4 class="modal-title" id="myModalLabel">Motivo del retiro</h4>
        </div>
        <div class="mymodal-body">
            <form class="form-horizontal" id = "retiroForm" role="form">
              <div class="form-group">
                <label for="motivo" class="col-sm-2">Motivo</label>
                <div class="col-sm-10">
                  <select name="reasons_select" id="reasons_select">
                    {{{reasons_options}}}
                  </select>
                </div>
              </div>
              <div class = "form-group">
                <label for="detalle" class="col-sm-2">Descripción</label>
                <div class="col-sm-10">
                  <textarea  class="form-control medium-inline" id="detalle" name="detalle" maxlength="1000"></textarea>
                </div>
              </div>
            </form>
        </div>
        <div class="mymodal-footer">
          <center><button type="button" class="button btn btn-primary" id="save_backdrop">Guardar</button></center>
          <center><button type="button" class="button btn btn-primary" id="goback_backdrop">Atrás</button></center>
        </div>
    </div>
  </div>
  <!-- End modal retiro-->
  
  <script type="text/javascript" src="../amd/src/require-setup.js"></script>
  <script data-main="../amd/src/student_profile/amd_student_profile" src="../amd/scripts/require.js"></script>
{{/data}}
