<body>

  {{#data}}

  <div class="container">
      <form  id="ficha_estudiante" onsubmit="myFunction(); return false;">
        
        <input type= "text" class="hide" id="id_ases" name="id_ases" value="{{id_ases}}" />
        <input type= "text" class="hide" id="id_moodle" name="id_moodle" value="{{id_moodle}}" />
        
          <div class="row m-bottom-20 cabecera">
              <div class="col-md-4">
                
                  <h2>FICHA GENERAL DEL ESTUDIANTE</h2>
                  
                  <div class="row">
                      <label class="col-sm-4 col-md-4 control-label">Código</label>
                      <div class="col-sm-4">
                        <input type="text" class="tip" id="codigo" value="{{code}}" size="12" maxlength="12" required>
                      </div>
                      <div class="col-sm-4">
                        <button type="button" class="btn-primary btn-sm" name="search" id="search">
                            <span class=" button glyphicon glyphicon-search"></span> Buscar
                        </button>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="estado"  class="col-sm-4 col-md-4 control-label">Estado Icetex</label>
                      <div class="col-sm-6">
                        <select name="estado" id="estado" class="col-md-5 form-control" disabled>
                          {{{status_icetex}}}
                        </select>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="estadoAses" class="col-md-4 control-label">Estado Ases</label>
                      <div class="col-sm-6">
                        <select name="estado_ases" id="estadoAses" class="col-md-5 form-control" disabled>
                            {{{status_ases}}}
                        </select>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="input_facultad" class="col-sm-4 control-label">Facultad</label>
                      <div class="col-sm-6">
                        <span id="input_facultad">{{faculty}}</span>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="input_programa" class="col-sm-4 control-label">Programa</label>
                      <div class="col-sm-6">
                        <span id="input_programa">{{program}}</span>
                      </div>
                  </div>
              </div><!-- End col-md-4 -->
  
              <div class="col-md-6">
                
                  <div class="row m-bottom-20">
                      <label class="col-md-12 nombre-ficha" id = "nombreficha">{{firstname}} {{lastname}}</label>
                  </div>
                  
                  <div class="row">
                      <label for="cedula" class="col-sm-2 control-label">Documento</label>
                      <div class="col-sm-6">
                        <input type="text" class="form-control" name="num_doc" id="cedula" value="{{num_doc}}" size="12" maxlength = "11" readonly>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="tipo_doc" class="col-sm-2 control-label">Tipo</label>
                      <div class="col-sm-6">
                        <select name="tipo_doc" id="tipo_doc" name="tipo_doc" disabled>
                           <option value="T.I" {{doc_type_ti}}>T.I.</option>
                           <option value="C.C" {{doc_type_cc}}>C.C.</option>
                           <option value="C.R" {{doc_type_cr}}>C.R.</option>
                           <option id="EMPTY" {{doc_type_empty}}></option>
                        </select>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="age" class="col-sm-2" >Edad:</label> 
                      <div class="col-sm-6">
                        <span id="age">{{age}}</span>
                      </div>
                  </div>
                  
                  <div class="row">
                      <label for="email1" class="col-sm-2 control-label" >Correo</label> 
                      <div class="col-sm-6">
                        <span id="email">{{email_moodle}}</span>
                      </div>
                  </div>

                  <div class="row">
                      <label class="col-sm-2" >Cohorte</label>
                      <div class="col-sm-6">
                        <span type="text" id ="fcohorte" name= "fcohorte" >{{cohort}}</span>
                      </div>
                  </div>
              </div>
              
              <div class="col-md-2 col-sm-2 talentos_ficha">
                      <img id="photo" class="img-rounded" src="../../../user/pix.php/{{id_user}}/f1.jpg" style="width:150px; height:180px">
              </div>
          </div>
  
