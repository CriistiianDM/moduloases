    <div class="col-sm-6" id="div-summary-it">
        <div class="panel panel-success no-border-radius">
            <div class="panel-heading">Ingeniería Topográfica</div>
            <div class="panel-body">
                Activos:
            </div>
        </div>
    </div>

    <div class="col-sm-6" id="div-summary-oa">
        <div class="panel panel-success no-border-radius">
            <div class="panel-heading">Otros ases</div>
            <div class="panel-body">
                Activos:
            </div>
        </div>
    </div>
</div>


    